{
	"$schema": "https://zed.dev/schema/themes/v0.1.0.json",
	"author": "fangorodev",
	"name": "mangoes",
	"themes": [
		{
			"appearance": "dark",
			"name": "Mangoes",
			"style": {
				"background": "#071018",
				"editor.background": "#071018",
				"editor.foreground": "#DDEBF0",
				"editor.line_number": "#6f858b",
				"editor.active_line.background": "#0b2530",
				"editor.selection.background": "#18343a",

				"border": "#0b1418",
				"panel.background": "#0b1418",
				"status_bar.background": "#091418",
				"tab_bar.background": "#071018",
				"tab.active_background": "#0f2226",
				"tab.inactive_background": "#071018",

				"accent": "#35B780",
				"scrollbar.thumb.background": "#15303a",


				"syntax": {
					"keyword": {
						"color": "#35B780",
						"font_weight": 600
					},
					"function": {
						"color": "#FF6F6F" 
					},
					"type": {
						"color": "#50D998"
					},
					"string": {
						"color": "#C47AC0"
					},
					"comment": {
						"color": "#6f8a84",
						"font_style": "italic"
					},
					"number": {
						"color": "#FFDFA3"
					},
					"operator": {
						"color": "#7EC8FF"
					},
					"variable": {
						"color": "#CFE9DB"
					},
					"property": {
						"color": "#A9E7C8"
					},
					"constant": {
						"color": "#FFB07A"
					},
					"boolean": {
						"color": "#F50305"
					},
					"punctuation": {
						"color": "#DDEBF0"
					}
				}

			}
		}
	]
}

